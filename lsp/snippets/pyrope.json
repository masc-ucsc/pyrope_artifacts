{   
	"For Loop": {
        "prefix": "for",
        "body":"for ${1:var}:(${2:val1} .. ${3:val2})(\n\t$0\n)",
        "description": "For loop"
    },
    "For Loop two condition": {
        "prefix": "for",
        "body":"for ${1:var}:(${2:val1} .. ${3:val2}) ${4: var2}:(${5:val1}, ${3:val2}) (\n\t$0\n)",
        "description": "For loop - two condition"
	},
    "For Loop iterate": {
        "prefix": "for",
        "body": "for ${1:var} in ${3:condition}(\n\t$0\n)",
        "description": "For Loop - Iterate"
    },
    "If block": {
        "prefix": "if",
        "body":[
            "if ${1:condition}(",
                "\t$0",
            ") elif ${3:condition}(",
                "\t$0",
            ") else (\n\t$0\n)"
        ],
        "description":"If block"
    },
    "While loop":{
        "prefix":"while",
        "body":"while ${1:var} ${2:!=|==|<|>|<=|>=} ${3:value}(\n\t$0\n)",
        "description":"While loop"
    },
    "Try block":{
        "prefix":"try",
        "body":[
            "try {\n\t$0\n}"
        ],
        "description":"Try block"
	},
	"Assert block":{
        "prefix":"I",
        "body":[
            "I(\n\t$0\n)"
        ],
        "description":"Assert block"
	},
	"function block":{
		"prefix":"::",
		"boly":[
			"::{\n\t$0\n}"
		],
		"description":"function block"
	}
}